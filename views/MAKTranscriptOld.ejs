          <hr class="borderr-2 mt-0 mb-1 me-1">

          <div class="p-0 results">
            <div class="row gx-0">
              <% studentData.chunkResults.forEach((chunkResult, chunkIndex) => { %>
                <div class="col-6 pe-1">
                  <% chunkResult.forEach((semester) => { %>
                    <div class="border rounded bg-light px-1 py-0 fw-bold text-sm mb-1">
                      <div class="mb-2">
                          <span class="me-4">Year: <%= semester.programme_study_year.replace('YEAR ', '') %></span>
                          <span class="mx-4">Academic Year :<%=  semester.academic_year %></span>
                          <span class="text-capitalize ms-4"><%= semester.semester %></span>
                        </div>
                        <div>
                          <span>Course Code</span>
                          <span>Course Name</span>
                          <span class="float-end me-1">Grade</span>
                          <span class="float-end me-1">CU</span>
                        </div>
                    </div>
                    <table
                      class="table table-sm table-borderless w-100 ms-2 mb-1"
                    >
                      <tbody>
                        <% semester.results.forEach((result) => { %>
                          <tr>
                            <td width="50"><%= result.course_unit_code %></td>
                            <td><%= result.course_unit_name %></td>
                            <td width="20"><%= parseFloat(result.credit_unit).toFixed(1) %> <div class="cu-circle">K</div> </td>
                            <td width="40">
                              <%= result.grading_letter %>
                              <% if (result.remark === 'CP') { %> <strong>(CP)</strong> <% } %>
                              <% if (result.remark === 'RT') { %> <strong>(RT)</strong> <% } %> 
                              <% if (result.is_audited_course === true || result.course_unit_category.includes('AUDITED') || result.is_audited_result) { %> <strong>(AC)</strong> <% } %>
                            </td>
                          </tr>         
                        <% }); %>


                        <tr>
                          <td class="text-center" colspan="4">
                            <span class="me-5"><strong>GPA: </strong> <%= semester.current_gpa  %></span>                          
                            <span><strong>CGPA: </strong><%= semester.cgpa  %></span>                          
                          </td>
                        </tr>
                      </tbody>
                    </table>
                    
                  <% }); %> 

                  <% if (chunkIndex === studentData.chunkResults.length - 1) { %>
                    <table class="table table-sm table-borderless m-0 text-md">
                      <tbody>
                        <tr>
                          <td class="pt-1"><strong>Minimum Graduation Load (CUs): 120</strong> <strong class="ms-3">Total Credit Units Accumulates:</strong> <%= studentData.totalCumulativeCUs %> </td>
