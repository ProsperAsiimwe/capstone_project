        <div class="mb-1">Office of the Academic Registrar, P.O. Box 7062, Kampala, Uganda.</div>
        <div>
          <strong>Tel</strong>: +256-414-538130/533332 
          <span class="dot"></span>
          <strong>Fax</strong>: +256-414-534125
          <span class="dot"></span>
          <strong>E-mail</strong>: ar@acadreg.mak.ac.ug
          <span class="dot"></span>
          <strong>Web</strong>: www.ar.mak.ac.ug
        </div>
      </div>
    </div>

      <% studentData.studyYearKeys.forEach((studyYears, index) => { %>
